
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Mosaddek">
    <meta name="keyword" content="FlatLab, Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <link rel="shortcut icon" href="~/Content/img/favicon.png">

    <title>Product list</title>

    <!-- Bootstrap core CSS -->

    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/css/bootstrap-reset.css" rel="stylesheet">
    <!--external css-->
    <link href="~/Content/assets/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/assets/jquery-ui/jquery-ui-1.10.1.custom.min.css" rel="stylesheet" />





    <!--Date Time CSS -->
    
    @*<link href="~/Content/assets/bootstrap-datepicker/css/datepicker.css" rel="stylesheet" />
    <link href="~/Content/assets/bootstrap-timepicker/compiled/timepicker.css" rel="stylesheet" />
    <link href="~/Content/assets/bootstrap-colorpicker/css/colorpicker.css" rel="stylesheet" />
    <link href="~/Content/assets/bootstrap-datetimepicker/css/datetimepicker.css" rel="stylesheet" />*@
    <!--Date Time CSS -->






    <!-- Custom styles for this template -->
    <link href="~/Content/css/style.css" rel="stylesheet">
    <link href="~/Content/css/style-responsive.css" rel="stylesheet" />

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 tooltipss and media queries -->
    <!--[if lt IE 9]>
      <script src="~/Content/js/html5shiv.js"></script>
      <script src="~/Content/js/respond.min.js"></script>
    <![endif]-->
</head>

<body>

    <section id="container" class="">
        <!--header start-->
        <header class="header white-bg">

            <!--logo start-->
            <!--logo end-->
            <div class="nav notify-row" id="top_menu">
                <!--  notification start -->
                <ul class="nav top-menu">
                </ul>
            </div>
            <div class="top-nav">
                <div class="input-group m-bot15">
                    <ul class="nav pull-right top-menu">

                        <li style="text-align:right">
                            <input type="text" class="form-control search" placeholder="Ara" style="width:900px;">
                            <button class="btn btn-white" type="button">Ara</button>
                        </li>

                        <!-- user login dropdown start-->


                        <li class="dropdown">

                            @{
                                Html.RenderAction("Index", "Home");
                            }
                            @RenderSection("cart", false)
                        </li>

                        <!-- user login dropdown end -->
                    </ul>
                </div>
            </div>

        </header>
        <!--header end-->
        <!--main content start-->
        <section id="main-content">
            <section class="wrapper">
                <!-- page start-->
                <div class="row">
                    <div class="col-md-2">
                        <section class="panel">
                            <div class="panel-body">
                                <input type="text" placeholder="Arama" class="form-control">
                            </div>
                        </section>
                        <section class="panel">
                            <header class="panel-heading">
                                Kategoriler
                            </header>
                            @{Html.RenderAction("Index", "HomeCategory");}
                        </section>
                        <section class="panel">
                            <header class="panel-heading">
                                Fiyat Aralığı
                            </header>
                            <div class="panel-body sliders">
                                <div id="slider-range" class="slider"></div>
                                <div class="slider-info">
                                    <span id="slider-range-amount"></span>
                                </div>
                            </div>
                        </section>
                        <section class="panel">
                            <header class="panel-heading">
                                En Çok Satılanlar
                            </header>
                            <div class="panel-body">
                                <div class="best-seller">
                                    <article class="media">
                                        <a class="pull-left thumb p-thumb">
                                            <img src="~/Content/img/product1.jpg">
                                        </a>
                                        <div class="media-body">
                                            <a href="#" class=" p-head">Item One Tittle</a>
                                            <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
                                        </div>
                                    </article>
                                    <article class="media">
                                        <a class="pull-left thumb p-thumb">
                                            <img src="~/Content/img/product2.png">
                                        </a>
                                        <div class="media-body">
                                            <a href="#" class=" p-head">Item Two Tittle</a>
                                            <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
                                        </div>
                                    </article>
                                    <article class="media">
                                        <a class="pull-left thumb p-thumb">
                                            <img src="~/Content/img/product3.png">
                                        </a>
                                        <div class="media-body">
                                            <a href="#" class=" p-head">Item Three Tittle</a>
                                            <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
                                        </div>
                                    </article>
                                </div>
                            </div>
                        </section>
                    </div>
                    @RenderBody()
                </div>
                <!-- page end-->
            </section>
        </section>
        <!--main content end-->
        <!--footer start-->
        <footer class="site-footer">
            <div class="text-center">
                2020 &copy; E-Ticaret
                <a href="#" class="go-top">
                    <i class="icon-angle-up"></i>
                </a>
            </div>
        </footer>
        <!--footer end-->


    </section>

    <!-- js placed at the end of the document so the pages load faster -->
    <script src="~/Content/js/jquery.js"></script>
    <script src="~/Content/js/bootstrap.min.js"></script>
    <script class="include" type="text/javascript" src="~/Content/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="~/Content/js/jquery.scrollTo.min.js"></script>
    <script src="~/Content/js/jquery.nicescroll.js" type="text/javascript"></script>
    <script src="~/Content/assets/jquery-knob/js/jquery.knob.js"></script>
    <script src="~/Content/assets/jquery-ui/jquery-ui-1.10.1.custom.min.js" type="text/javascript"></script>
    <script src="~/Content/js/jquery.ui.touch-punch.min.js"></script>
    <script src="~/Content/js/jquery.customSelect.min.js"></script>
    <script src="~/Content/js/jquery.validate.min.js"></script>
    <script src="~/Content/js/respond.min.js"></script>
    <script src="~/Content/js/jquery.validate.min.js"></script>


    <!--DateTime JS-->
    @*<script src="~/Content/assets/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>
        <script src="~/Content/assets/bootstrap-datetimepicker/js/bootstrap-datetimepicker.js"></script>*@
    <!--DateTime JS-->


    <script src="~/Content/js/form-validation-script.js"></script>
    <!--common script for all pages-->
    <script src="~/Content/js/common-scripts.js"></script>

    <script src="~/Content/js/sliders.js" type="text/javascript"></script>
    <script src="~/Content/assets/bootstrap-inputmask/bootstrap-inputmask.min.js"></script>
    <script>
//knob
      $(".knob").knob();</script>
    <script type="text/javascript">
$(document).ready(function() {

              $(function(){
                  $('select.styled').customSelect();
              });
          });</script>

</body>
</html>
